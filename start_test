#!/bin/bash
bash compile

python3 test_split.py

echo "Successfully split!"

time for file in ./tests/*; do echo -e '\n'$file &>> output_a.txt; ./a < $file &>> output_a.txt; done

echo "Program a executed!"

time for file in ./tests/*; do echo -e '\n'$file &>> output_b.txt; ./b < $file &>> output_b.txt; done

echo "Program b executed!"

diff -y output_a.txt output_b.txt >> results.txt
diff -q output_a.txt output_b.txt >> results.txt

cat results.txt

